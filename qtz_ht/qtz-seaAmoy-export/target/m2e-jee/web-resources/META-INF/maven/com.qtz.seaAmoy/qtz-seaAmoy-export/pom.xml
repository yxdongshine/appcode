			</dependency>
			<dependency>
		     	<groupId>com.qtz.dongmeng</groupId>
		  		<artifactId>qtz_dm_service_api</artifactId>
		  		<version>0.0.1-SNAPSHOT</version>
		  		<type>jar</type>
		  		<exclusions>
		  			<exclusion>
		  			<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-starter-web</artifactId>
		  			</exclusion>
		  		</exclusions>
           </dependency>
  </dependencies>
  <build>
    <finalName>qtz-seaAmoy-export</finalName>
     <resources>
          <resource>
              <directory>src/main/resources</directory>
              <includes>
                  <include>**/*</include>
              </includes>
              <filtering>true</filtering>
          </resource>
      </resources>
  </build>
  
    <profiles>
        <profile>
            <id>dev</id>
               <activation>
               <!--默认激活开发配制-->
               <activeByDefault>true</activeByDefault>
            </activation>
            <build>
                <filters>
                    <filter>src/main/filters/dev.properties</filter>
                </filters>
            </build>
        </profile>
        <profile>
            <id>test</id>
            <build>
                <filters>
                    <filter>src/main/filters/test.properties</filter>
                </filters>
            </build>
        </profile>
        <profile>
            <id>pro</id>
            <build>
                <filters>
                    <filter>src/main/filters/pro.properties</filter>
                </filters>
            </build>
        </profile>
    </profiles>
    <properties>
	  <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
</project>