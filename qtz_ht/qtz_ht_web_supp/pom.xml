<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.qtz.seaAmoy</groupId>
    <artifactId>qtz_ht</artifactId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  
  <artifactId>qtz_ht_web_supp</artifactId>
  <packaging>war</packaging>
  <name>qtz_ht_web_supp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
	<dependency>
     	<groupId>com.qtz.seaAmoy</groupId>
  		<artifactId>qtz_ht_session_api</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
  		<type>jar</type>
  		<exclusions>
  			<exclusion>
  			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
  			</exclusion>
  		</exclusions>
    </dependency>
  	<dependency>
     	<groupId>com.qtz.seaAmoy</groupId>
  		<artifactId>qtz_ht_service_api</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
  		<type>jar</type>
  		<exclusions>
  			<exclusion>
  			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
  			</exclusion>
  		</exclusions>
    </dependency>
	<dependency>
       <groupId>com.qtz.order</groupId>
 	   <artifactId>qtz_order_service_api</artifactId>
       <version>0.0.1-SNAPSHOT</version>
		<type>jar</type>
		<exclusions>
			<exclusion>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-starter-web</artifactId>
			</exclusion>
		</exclusions>
	</dependency>
	<dependency>
	    <groupId>com.aliyun.oss</groupId>
	    <artifactId>aliyun-sdk-oss</artifactId>
	    <version>2.0.2</version>
	</dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <scope>test</scope>
    </dependency>
     <!-- mysql的数据库驱动包 -->
<!-- 	<dependency> -->
<!-- 		<groupId>mysql</groupId> -->
<!-- 		<artifactId>mysql-connector-java</artifactId> -->
<!-- 	</dependency> -->
    <dependency>
			<groupId>com.mangofactory</groupId>
			<artifactId>swagger-springmvc</artifactId>
	</dependency>
  
  </dependencies>
  <build>
    <finalName>qtz_ht_web_supp</finalName>
     <resources>
          <resource>
              <directory>src/main/resources</directory>
              <includes>
                  <include>**/*</include>
              </includes>
              <filtering>true</filtering>
          </resource>
      </resources>
  </build>
  
    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <!--默认激活开发配制-->
                <activeByDefault>true</activeByDefault>
            </activation>
            <build>
                <filters>
                    <filter>src/main/filters/dev.properties</filter>
                </filters>
            </build>
        </profile>
        <profile>
            <id>test</id>
            <build>
                <filters>
                    <filter>src/main/filters/test.properties</filter>
                </filters>
            </build>
        </profile>
        <profile>
            <id>pro</id>
            <build>
                <filters>
                    <filter>src/main/filters/pro.properties</filter>
                </filters>
            </build>
        </profile>
    </profiles>
    <properties>
	  <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
</project>